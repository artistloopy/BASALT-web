---
import logoPng from '~/assets/images/logo.png?url';
// fallback to original svg/icon if needed
// import favIcon from '~/assets/favicons/favicon.ico';
// import favIconSvg from '~/assets/favicons/favicon.svg';
// import appleTouchIcon from '~/assets/favicons/apple-touch-icon.png';
---

<link rel="shortcut icon" href={logoPng} />
<link rel="icon" type="image/png" href={logoPng} />
<link rel="mask-icon" href={logoPng} color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={logoPng} />
